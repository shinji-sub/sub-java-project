package Lambda;

public class Lambda {
  public static void main(final String[] args) {
    // MyFunction을 만들고 사용
    // 함수형 인터페이스
    // 람다 사용 전
    MyFunction f  = new MyFunction() {
      public int max(int a, int b) {
        return a > b ? a: b;
      }
    };
    System.out.println(f.max(3, 4));
  }
}

