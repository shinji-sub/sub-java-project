package Lambda;

import java.util.Arrays;

public class Lambda {
  public static void main(String[] args) {
    // 람다식 작성
    // 
    int[] arr = new int[10];
    Arrays.setAll(arr, (i) -> (int)Math.random()*45+1);
    for (int i = 0; i < 10; i++) {
      System.out.printf("%d ", arr[i]);
    }
    
  }
}
